{
  "name": "gmbf-package/cloudflare-image-upload",
  "description": "Provides an easy-to-use interface to upload, delete, and fetch images from Cloudflare Images API in Laravel projects.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.0 || ^8.0",
    "illuminate/support": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "guzzlehttp/guzzle": "^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Gmbf\\CloudflareImageUpload\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gmbf\\CloudflareImageUpload\\CloudflareImageServiceProvider"
      ],
      "aliases": {
        "CloudflareImage": "Gmbf\\CloudflareImageUpload\\Facades\\CloudflareImage"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
