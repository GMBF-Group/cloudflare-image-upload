{
    "name": "gmbf/cloudflare-image-upload",
    "description": "Provides an easy-to-use interface to upload and manage images on Cloudflare from Laravel applications.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.4 <8.4",
        "illuminate/support": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Gmbf\\CloudflareImageUpload\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gmbf\\CloudflareImageUpload\\CloudflareImageUploadServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
